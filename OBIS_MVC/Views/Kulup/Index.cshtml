@using OBIS_MVC.Models.Entity_Framework;
@model List<TBL_KULUP>
@{

    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<h2 style="margin-top:15px; text-align:center;">Kulüp Listesi</h2>
<hr />
<a style="margin-bottom:15px;" href="#" class="btn btn-primary">Kulüp Ekle</a>
<table class="myList">
    <thead>
        <tr style="text-align:center;">
            <th scope="col">KULÜP AD</th>
            <th scope="col">KULÜP KONTENJAN</th>
            <th scope="col">İŞLEMLER</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr style="text-align:center;">
            <td>@item.KLPAD</td>
            <td>@item.KLPKONTENJAN</td>

            <td>
                <a href="#" class="btn btn-warning">Güncelle</a>
                <a href="#" class="btn btn-danger">Sil</a>

            </td>
        </tr>
        }
    </tbody>
</table>